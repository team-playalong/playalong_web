{
  "name": "playalongweb",
  "private": false,
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "watch": "npm-run-all --parallel test:watch start",
    "start": "NODE_ENV=development webpack-dev-server --port 3000 --progress --config webpack.config.dev.js",
    "tslint": "tslint './app/**/*.ts' --fix",
    "eslint": "eslint --fix test",
    "deploy": "firebase use default && firebase deploy",
    "prebuild": "yarn && jest",
    "build": "webpack -p --progress --config webpack.config.build.js",
    "ci": "yarn build && yarn deploy",
    "clean": "rm -rf bower_components && rm -rf node_modules && npm cache clean"
  },
